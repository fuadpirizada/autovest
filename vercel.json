{
  "version": 2,
  "buildCommand": "npm run build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/$1"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ],
  "env": {
    "SESSION_SECRET": "@autovest-session-secret",
    "STRIPE_SECRET_KEY": "@autovest-stripe-secret-key",
    "VITE_STRIPE_PUBLIC_KEY": "@autovest-stripe-public-key"
  }
}